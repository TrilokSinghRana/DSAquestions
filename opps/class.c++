#include <iostream>
using namespace std;

void introduce(string name, int age) {
    cout << name << " " << age << endl;
}

int main() {
    introduce("Amit", 20);
    introduce("Priya", 21);
    introduce("Rahul", 22);
    introduce("Ami", 20);
    introduce("Pria", 21);
    introduce("Rahl", 22);
}
